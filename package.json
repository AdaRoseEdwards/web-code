{
  "name": "web-code",
  "version": "1.0.2",
  "description": "Text editor for the web",
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "webcode": "bin/webcode"
  },
  "scripts": {
    "build": "rollup static/scripts/main.js --format iife --sourcemap inline --output static/scripts/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "./node_modules/.bin/nodemon --ignore static/scripts/bundle.js --exec \"npm run build; npm run start\"",
    "start": "node index.js"
  },
  "author": "Ada Rose Edwards",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "file-icons": "git+https://github.com/file-icons/atom.git",
    "mime": "^1.3.4",
    "monaco": "^1.201704190613.0",
    "monaco-editor": "^0.8.3",
    "ws": "^2.3.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "reload": "^1.1.3",
    "rollup": "^0.41.6",
    "rollup-plugin-sourcemaps": "^0.4.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:import/errors"
    ],
    "parserOptions": {
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "mocha": true,
      "node": true
    },
    "rules": {
      "no-unused-vars": 2,
      "no-undef": 2,
      "eqeqeq": 2,
      "no-underscore-dangle": 0,
      "guard-for-in": 2,
      "no-extend-native": 2,
      "wrap-iife": 2,
      "new-cap": 2,
      "no-caller": 2,
      "quotes": [
        1,
        "single"
      ],
      "no-loop-func": 2,
      "no-irregular-whitespace": 1,
      "no-multi-spaces": 2,
      "one-var": [
        2,
        "never"
      ],
      "no-var": 1,
      "strict": [
        1,
        "global"
      ],
      "no-console": 1
    },
    "root": true
  }
}
